# Google Cloud Configuration
GOOGLE_CLOUD_PROJECT=your-project-id
GOOGLE_CLOUD_LOCATION=us-central1
GOOGLE_APPLICATION_CREDENTIALS=./config/service-account-key.json

# Storage Configuration
STORAGE_BUCKET=appraisily-generated-images

# ImageKit Configuration
# In production, IMAGEKIT_PRIVATE_KEY should be accessed via Secret Manager as IMAGEKIT_API_KEY
IMAGEKIT_PUBLIC_KEY=your-imagekit-public-key
IMAGEKIT_PRIVATE_KEY=your-imagekit-private-key
IMAGEKIT_URL_ENDPOINT=https://ik.imagekit.io/appraisily
IMAGEKIT_FOLDER=appraiser-images

# Image Generation Configuration
# Vertex AI model name for image generation
IMAGEN_MODEL=imagegeneration@002

# OpenAI Configuration for GPT-4o
# In production, this is accessed via Secret Manager as OPEN_AI_API_SEO
# Only set this in the .env file for local development
OPEN_AI_API_SEO=your-openai-api-key

# Service Configuration
PORT=3000
LOG_LEVEL=INFO  # DEBUG, INFO, WARN, ERROR

# Secret Manager
# When deployed to Cloud Run, these secrets should be configured in the Cloud Run service settings
# DO NOT set these in the .env file in production
# OPEN_AI_API_SEO=projects/{project-id}/secrets/OPEN_AI_API_SEO/versions/latest
# IMAGEKIT_API_KEY=projects/{project-id}/secrets/IMAGEKIT_API_KEY/versions/latest 